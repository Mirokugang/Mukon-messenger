{
  "name": "mukon-messenger-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npx expo start --dev --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "./build-apk.sh debug",
    "build:clean": "./build-apk.sh debug clean",
    "build:prebuild": "npx expo prebuild --clean && ./build-apk.sh debug",
    "build:release": "./build-apk.sh release",
    "build:release:clean": "./build-apk.sh release clean",
    "build:release:prebuild": "npx expo prebuild --clean && ./build-apk.sh release",
    "deploy": "adb install -r mukon-debug.apk",
    "deploy:release": "adb install -r mukon-release.apk",
    "dev": "concurrently \"npm run android:dev\" \"npm run start --dev-client\""
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@craftzdog/react-native-buffer": "^6.1.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@solana-mobile/mobile-wallet-adapter-protocol": "^2.2.5",
    "@solana-mobile/mobile-wallet-adapter-protocol-web3js": "^2.2.5",
    "@solana/web3.js": "^1.95.0",
    "assert": "^2.1.0",
    "bs58": "^5.0.0",
    "buffer": "^6.0.3",
    "expo": "~51.0.0",
    "expo-clipboard": "~6.0.3",
    "expo-status-bar": "~1.12.1",
    "js-base64": "^3.7.8",
    "js-sha256": "^0.11.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-get-random-values": "~1.11.0",
    "react-native-paper": "^5.12.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-worklets-core": "^1.6.2",
    "readable-stream": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "text-encoding-polyfill": "^0.6.7",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "~18.2.45",
    "babel-plugin-module-resolver": "^5.0.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
