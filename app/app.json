{
  "expo": {
    "name": "Mukon Messenger",
    "slug": "mukon-messenger",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../ICON.png",
    "splash": {
      "image": "../Logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "react-native-reanimated/plugin"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mukon.messenger"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../ICON.png",
        "backgroundColor": "#0D0D0D"
      },
      "package": "com.mukon.messenger"
    },
    "web": {
      "favicon": "../ICON.png"
    },
    "scheme": "mukon"
  }
}
